local int newWalkSpeed = 16;

local player = game:GetService("Players").localPlayer
local char = player.Character
local humanoid = char.Humanoid

SpeedInput.FocusLost:Connect(function(enter)
		if enter then
				local int newWalkSpeed = SpeedInput.Text
				setSpeed(newWalkSpeed)
		end
end)
--------------------------------- Functions ---------------------------------

local game_meta = getrawmetatable(game)

setreadonly(game_meta, false)

local oldIndex = game_meta.__index

-- WalkSpeed --

function setSpeed(newWalkSpeed)
    game_meta.__index = function(self, b)
        num = newWalkSpeed + 0
        if b == "WalkSpeed" then
            return 16
        end
        return oldIndex(self, b)
    end
    humanoid.WalkSpeed = num;
end


UICorner_15.Parent = SpeedInput

