local function WMHT_fake_script() -- PlayerName.Username 
	local script = Instance.new('LocalScript', PlayerName)

	local player = game.Players.LocalPlayer

	if player then
		script.Parent.Text = ""..player.Name..""
	end
end
coroutine.wrap(WMHT_fake_script)()
local function QXKVC_fake_script() -- PlayerImage.LocalScript 
	local script = Instance.new('LocalScript', PlayerImage)

	local frame = script.Parent.Parent
	local player = game.Players.LocalPlayer
	local userId = player.UserId
	local thumType = Enum.ThumbnailType.HeadShot
	local thumSize = Enum.ThumbnailSize.Size180x180
	local content = game.Players:GetUserThumbnailAsync(userId, thumType, thumSize)

	frame.PlayerImage.Image = content
	frame.PlayerName.Text = player.Name
end
coroutine.wrap(QXKVC_fake_script)()
local function OTXXLD_fake_script() -- CloseButton.CloseUI 
	local script = Instance.new('LocalScript', CloseButton)

	local ScreenGui = script.Parent.Parent.Parent

	script.Parent.MouseButton1Click:Connect(function()
		ScreenGui:Destroy()
	end)
end
coroutine.wrap(OTXXLD_fake_script)()
local function PEXMNOP_fake_script() -- MinimizeButton.MinimizeUI 
	local script = Instance.new('LocalScript', MinimizeButton)

	local mainFrame = script.Parent.Parent
	local open = false

	script.Parent.MouseButton1Click:Connect(function()
		if mainFrame.Visible == false then
			mainFrame.Visible = true
		else
			mainFrame.Visible = false
		end
	end)
end
coroutine.wrap(PEXMNOP_fake_script)()
local function QWFGQFJ_fake_script() -- Main.KeybindMinimize 
	local script = Instance.new('LocalScript', Main)

	local mainFrame = script.Parent
	local open = false
	local UIS = game:GetService("UserInputService")

	UIS.InputBegan:Connect(function(keycode)
		if keycode.KeyCode == Enum.KeyCode.RightAlt then
			if mainFrame.Visible == false then
				mainFrame.Visible = true
			else
				mainFrame.Visible = false
			end
		end
	end)

end
coroutine.wrap(QWFGQFJ_fake_script)()
