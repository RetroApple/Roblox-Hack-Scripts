local function IVBFHLG_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local enabled = false

	button.MouseButton1Click:Connect(function()
		if enabled == false then
			enabled = true
			button.Text = "ON"
			button.TextColor3 = Color3.fromHex("47ce0c")
			--button.TextColor = Color3.new(71, 206, 12)
		else
			enabled = false
			button.Text = "OFF"
			button.TextColor3 = Color3.fromHex("ce2908")
			--button.TextColor3 = Color3.new(206, 41, 8)
		end
	end)


end
coroutine.wrap(IVBFHLG_fake_script)()
