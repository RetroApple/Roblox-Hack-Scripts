------------------------ ESPButton ----------------------------

ESPButton.Name = "ESPButton"
ESPButton.Parent = ESPBorder
ESPButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ESPButton.BackgroundTransparency = 1.000
ESPButton.BorderSizePixel = 0
ESPButton.Position = UDim2.new(0.5625, 0, 0, 0)
ESPButton.Size = UDim2.new(0, 43, 0, 50)
ESPButton.Font = Enum.Font.SourceSansBold
ESPButton.Text = "OFF"
ESPButton.TextColor3 = Color3.fromRGB(206, 41, 8)
ESPButton.TextSize = 24.000

local espSettings = {
	textsize = 20,
	color = 255, 255, 255
}

_G.chams = false
local Players = game:GetService("Players")
-- local billboardESPGUI = Instance.new("BillboardGui")
-- local ESPText = Instance.new("TextLabel", billboardESPGUI)

ESPButton.MouseButton1Click:Connect(function()
	if _G.chams == false then
		_G.chams = true
		ESPButton.Text = "ON"
		ESPButton.TextColor3 = Color3.fromHex("47ce0c")

		function CreateGui(name,parent,face) -- Creating the _G.chams
			local SurfaceGui = Instance.new("SurfaceGui",parent)
			SurfaceGui.Parent = parent
			SurfaceGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
			SurfaceGui.Face = Enum.NormalId[face]
			SurfaceGui.LightInfluence = 0
			SurfaceGui.ResetOnSpawn = false
			SurfaceGui.Name = name
			SurfaceGui.AlwaysOnTop = true
			local Frame = Instance.new("Frame",SurfaceGui)
			Frame.BackgroundColor3 = Color3.fromRGB(85, 170, 255) -- Color for the _G.chams
			Frame.Size = UDim2.new(1,0,1,0)
		end
		
		local billboardESPGUI = Instance.new("BillboardGui")
		local ESPText = Instance.new("TextLabel", billboardESPGUI)
		
		
		billboardESPGUI.Name = "RetroESP"
		billboardESPGUI.ResetOnSpawn = false
		billboardESPGUI.AlwaysOnTop = true
		billboardESPGUI.LightInfluence = 0
		billboardESPGUI.Size = UDim2.new(1, 0, 1.75, 0)
		ESPText.BackgroundTransparency = 1
		ESPText.Text = ""
		ESPText.Size = UDim2.new(1, 0, 1, 0)
		ESPText.BorderSizePixel = 4
		ESPText.BorderColor3 = Color3.new(espSettings.color)
		ESPText.BorderSizePixel = 0
		ESPText.Font = "SourceSansBold"
		ESPText.TextSize = espSettings.textsize
		ESPText.TextColor3 = Color3.fromRGB(255, 255, 255)


		   while _G.chams == true do
		     for i,v in pairs (game:GetService("Players"):GetPlayers()) do
		         if v ~= game:GetService("Players").LocalPlayer and v.Character.Head:FindFirstChild("RetroESP")==nil then
		             ESPText.Text = "{"..v.Name.."}"
		             billboardESPGUI:Clone().Parent = v.Character.Head
		       end
			end
			wait(0.25)
		   end
		
	else 
		_G.chams = false
		ESPButton.Text = "OFF"
		ESPButton.TextColor3 = Color3.fromHex("ce2908")
		print("OFF")

		while _G.chams == false do
			for i, v in pairs (Players:GetPlayers()) do
				for i, v in pairs (v.Character:GetDescendants()) do
					if v:IsA("SurfaceGui") or v.Name == "RetroESP" then
						v:Destroy()
					end
				end
			end
			wait(0.25)
		end
		
	end -- IF STATEMENT "END"
	
	while _G.chams == true do
		print("DPES THIS OWRK???")
		for i,v in pairs (Players:GetPlayers()) do --- gets all the players in the game and loops through them
			if v ~= Players.LocalPlayer and v.Character ~= nil and v.Character:FindFirstChild("Head") and _G.chams and v.Character.Head:FindFirstChild("cham") == nil then --- Checks to check if the player is appropiate to make a cham
				for i,v in pairs (v.Character:GetChildren()) do -- looping through every child in the character of the player
					if v:IsA("MeshPart") or v.Name == "Head" or v:IsA("Part") then -- checking if the child is a body part
						CreateGui("cham",v,"Back")
						CreateGui("cham",v,"Front")
						CreateGui("cham",v,"Top")
						CreateGui("cham",v,"Bottom")
						CreateGui("cham",v,"Right")
						CreateGui("cham",v,"Left")
					end
				end
			end
		end
		wait(0.25) 
	end
end)



